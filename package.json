{
  "name": "markdown-it-incremental-dom",
  "version": "0.0.0",
  "description": "markdown-it renderer plugin by using Incremental DOM.",
  "main": "lib/markdown-it-incremental-dom.js",
  "browser": "dist/markdown-it-incremental-dom.js",
  "author": {
    "name": "Yuki Hattori",
    "url": "https://github.com/yhatt"
  },
  "keywords": [
    "markdown-it-plugin",
    "markdown-it",
    "markdown",
    "incremental-dom"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yhatt/markdown-it-incremental-dom"
  },
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:commonjs": "npm run clean:lib && babel src --out-dir lib",
    "build:browser": "npm run clean:dist && webpack",
    "clean": "npm-run-all --parallel clean:*",
    "clean:lib": "rimraf lib",
    "clean:dist": "rimraf dist",
    "lint": "eslint --cache .",
    "lint:nocache": "eslint .",
    "test": "NODE_ENV=test mocha"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb-base": "^10.0.1",
    "eslint-plugin-import": "^2.2.0",
    "incremental-dom": "^0.5.1",
    "json-loader": "^0.5.4",
    "markdown-it": ">=4.0.0",
    "mocha": "^3.2.0",
    "npm-run-all": "^3.1.2",
    "rimraf": "^2.5.4",
    "webpack": "^1.14.0"
  },
  "peerDependencies": {
    "markdown-it": ">=4.0.0"
  },
  "dependencies": {
    "htmlparser2": "^3.9.2"
  }
}
